{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "import/extensions":[
            "error",
            "ignorePackages",
            {
               "ts":"never",
               "tsx":"never",
               "js":"never",
                "jsx":"never"
            }
         ]
    },
    "settings":{
        "import/resolver":{
           "typescript":{
              
           }
        }
     }
}
